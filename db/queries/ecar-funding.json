{
  "constraints": [
    {
      "constraint_class": "entity_type",
      "constraint_fields": {
        "value": {
          "constraint_group": "string_constraint",
          "constraint_type": "exact",
          "constraint_features": {
            "exact": "person"
          }
        }
      }
    },
    {
      "constraint_class": "personal_data",
      "constraint_fields": {
        "drivers_license": {
          "constraint_group": "bool_constraint",
          "constraint_type": "match",
          "constraint_features": {
            "match": true
          }
        }
      }
    },
    {
      "constraint_class": "car",
      "constraint_fields": {
        "car_age": {
          "constraint_group": "int_constraint",
          "constraint_type": "max",
          "constraint_features": {
            "max": 20
          }
        },
        "car_type": {
          "constraint_group": "string_constraint",
          "constraint_type": "exact",
          "constraint_features": {
            "exact": "electric"
          }
        }
      }
    }
  ]
}

